package datenbankanbindung;

public class Sitzplatz {
	public int nr;
	public Zustand status;
	public String nameReservator;
	public boolean reservierungenAnnehmen;

	

	public Sitzplatz(int nr, Zustand status, String nameReservator, boolean reservierungenAnnehmen) {
		this.nr = nr;
		this.status = status;
		this.nameReservator = nameReservator;
		this.reservierungenAnnehmen = reservierungenAnnehmen;
	}

	public Sitzplatz(int nr, Zustand status, boolean reservierungenAnnehmen) {
		this.nr = nr;
		this.status = status;
		this.reservierungenAnnehmen = reservierungenAnnehmen;
	}
}
